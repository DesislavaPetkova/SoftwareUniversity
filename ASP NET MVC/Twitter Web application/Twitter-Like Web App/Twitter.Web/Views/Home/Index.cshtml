@using PagedList
@using PagedList.Mvc
@using Twitter.Models
@model PagedList.IPagedList<Twitter.Web.ViewModels.TweetViewModel>

@{
    ViewBag.Title = "Home Page";
}
<br />

@if (this.User.Identity.IsAuthenticated)
{
    Html.RenderPartial("Tweet/CreateTweetPartial");
}

<br />
<div class="row">
    @Html.DisplayFor(x => x)
</div>
@Html.PagedListPager(Model, page => Url.Action("Index", new { page }))